super + w
    chrome

super + Return
	alacritty

super + shift + Return
	rofi -show run

super + t
	time_date

super + p
	strtwebcam1

super + shift + p
	strtwebcam2

super + f
	alacritty -e fff

super + m
	alacritty -e cmus

super + e
	emoji-selector

super + shift + s
	scrot

super + g
	gimp

super + shift + r
	bspc wm -r

super + shift + q
	pkill picom dunst feh sxhkd bspwm

super + q
	bspc node -c

super + i
    bspc node -R 90
super + shift + i
    bspc node -R -90

super + {shift + t,shift + s,shift + f}
	bspc node -t {tiled,floating,fullscreen}

super + {_,shift + }{h,j,k,l}
	bspc node -{f,s} {west,south,north,east}

super + {_,shift + }{1-5}
	bspc {desktop -f,node -d} '^{1-5}'

super + ctrl + {h,j,k,l}
	bspc node -z {left -80 0,bottom 0 80,top 0 -80,right 80 0}

super + ctrl + shift + {h,j,k,l}
	bspc node -z {right -80 0,top 0 80,bottom 0 -80,left 80 0}
